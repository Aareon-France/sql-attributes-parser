{
  "name": "aareon-france/sql-attributes-parser",
  "description": "Parser of SQL attributes, which offers the ability to add metadata on SQL queries.",
  "type": "library",
  "authors": [
    {"name": "Giraud Nicolas","email": "nicolas.giraud@aareon.com"}
  ],
  "require": {
    "php": ">=8.3"
  },
  "require-dev": {
    "friendsofphp/php-cs-fixer": "3.56.1",
    "phpstan/extension-installer": "1.3.1",
    "phpstan/phpstan": "1.11.0",
    "phpstan/phpstan-deprecation-rules": "1.2.0",
    "phpstan/phpstan-phpunit": "1.4.0",
    "phpstan/phpstan-strict-rules": "1.6.0",
    "phpstan/phpstan-symfony": "1.4.0",
    "phpunit/phpunit": "10.5.20",
    "roave/security-advisories": "dev-latest",
    "vimeo/psalm": "5.24.0"
  },
  "autoload": {"psr-4": {"AareonFrance\\SqlAttributesParser\\": "src/SqlAttributesParser/"}},
  "autoload-dev": {"psr-4": {"AareonFrance\\SqlAttributesParser\\Tests\\": "src/Tests/"}},
  "scripts": {},
  "config": {
    "allow-plugins": {
      "phpstan/extension-installer": true
    },
    "platform": {"php": "8.3"},
    "preferred-install": {"*": "dist"},
    "optimize-autoloader": true,
    "sort-packages": true
  }
}
